<header class='container-fluid d-flex ht-100 mb-1'>
  <div class='container-fluid logo'>
    MOB
  </div>
  <div class='d-flex justify-content-end align-items-end btn-ff'>
      <% if user_signed_in? %>  
        <%= link_to current_user.nickname, '#', class: "head-btn" %>
        <%= link_to 'ログアウト', destroy_user_session_path, method: :delete, class: "head-btn" %>
      <% else %> 
        <%= link_to 'ログイン', new_user_session_path, class: "head-btn" %>
        <%= link_to '新規登録', new_user_registration_path, class: "head-btn" %>
      <% end %> 
    </div>
  </div>
</header>